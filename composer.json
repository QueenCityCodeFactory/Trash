{
    "name": "queencitycodefactory/trash",
    "description": "Adds soft delete support to CakePHP 3 ORM tables.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "trash",
        "orm"
    ],
    "homepage": "https://github.com/queencitycodefactory/trash",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Daren Sipes",
            "homepage": "https://github.com/darensipes",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/trash/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/queencitycodefactory/trash/issues",
        "source": "https://github.com/queencitycodefactory/trash"
    },
    "require": {
        "cakephp/orm": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "<6.0",
        "cakephp/cakephp": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Trash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Trash\\Test\\": "tests"
        }
    }
